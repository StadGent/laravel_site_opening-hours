{
    "name": "stadgent/laravel_site_opening-hours",
    "description": "An open source web application to create calendars that represent openinghours of public services.",
    "keywords": ["framework", "laravel"],
    "license": "AGPLv3",
    "type": "project",
    "require": {
        "php": ">=7.3.0",
        "digipolisgent/robo-digipolis-laravel": "^2.0.2",
        "doctrine/dbal": "^2.5",
        "easyrdf/easyrdf": "^0.9.1",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "^1.4",
        "grasmash/yaml-expander": "^1.1",
        "guzzlehttp/guzzle": "^7.3",
        "hedronium/spaceless-blade": "^3.0",
        "johngrogg/ics-parser": "^2.1.5",
        "laravel/framework": "^6.18.26",
        "laravel/passport": "^9.3.2",
        "laravel/tinker": "^1.0",
        "ml/json-ld": "^1.0",
        "nesbot/carbon": "^2.0",
        "pda/pheanstalk": "~4.0",
        "rlanvin/php-rrule": "^1.6",
        "santigarcor/laratrust": "5.2.*",
        "wildside/userstamps": "^0.5.3"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "laravel/browser-kit-testing": "2.*",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "nunomaduro/collision": "^3.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
         "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/BrowserKitTestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "vendor/bin/robo precommit:install",
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.3"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
