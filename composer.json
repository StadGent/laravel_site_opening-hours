{
    "name": "stadgent/laravel_site_opening-hours",
    "description": "An open source web application to create calendars that represent openinghours of public services.",
    "keywords": ["framework", "laravel"],
    "license": "AGPLv3",
    "type": "project",
    "require": {
        "php": ">=7.3.0",
        "digipolisgent/robo-digipolis-laravel": "^3.0",
        "doctrine/dbal": "^2.5",
        "easyrdf/easyrdf": "^0.9.1",
        "fzaninotto/faker": "^1.4",
        "guzzlehttp/guzzle": "^7.3",
        "hedronium/spaceless-blade": "^3.0",
        "johngrogg/ics-parser": "^2.1.5",
        "laravel/framework": "^9.0",
        "laravel/passport": "^10",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.2",
        "ml/json-ld": "^1.0",
        "nesbot/carbon": "^2.0",
        "pda/pheanstalk": "~4.0",
        "rlanvin/php-rrule": "^1.6",
        "santigarcor/laratrust": "^7",
        "spatie/laravel-ignition": "^1.0",
        "wildside/userstamps": "^2.3"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "laravel/browser-kit-testing": "^6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0",
        "nunomaduro/collision": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Openinghours\\": "robo"
        },
         "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/BrowserKitTestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
