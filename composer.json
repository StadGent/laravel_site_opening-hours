{
    "name": "StadGent/laravel_site_opening-hours",
    "description": "An open source web application to create calendars that represent openinghours of public services.",
    "keywords": ["framework", "laravel"],
    "license": "AGPLv3",
    "type": "Web application",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/weconnectdata/ics-parser"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.7.*",
        "santigarcor/laratrust": "3.0.*",
        "johngrogg/ics-parser": "^2.1.5",
        "nesbot/carbon": "^1.28",
        "doctrine/dbal": "^2.5",
        "easyrdf/easyrdf": "^0.9.1",
        "ml/json-ld": "^1.0",
        "pda/pheanstalk": "^3.1",
        "digipolisgent/robo-digipolis-laravel": "^0.1.3",
        "grasmash/yaml-expander": "^1.1",
        "digipolisgent/robo-digipolis-general": "0.1.1",
        "laravel/passport": "^7.0",
        "rlanvin/php-rrule": "^1.6",
        "fzaninotto/faker": "^1.4",
        "wildside/userstamps": "^0.5.3",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "laravel/browser-kit-testing": "2.*",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "nunomaduro/collision": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
         "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/BrowserKitTestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "vendor/bin/robo precommit:install",
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
