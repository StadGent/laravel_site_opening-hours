{
    "name": "StadGent/laravel_site_opening-hours",
    "description": "An open source web application to create calendars that represent openinghours of public services.",
    "keywords": ["framework", "laravel"],
    "license": "AGPLv3",
    "type": "Web application",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "santigarcor/laratrust": "3.0.*",
        "johngrogg/ics-parser": "dev-master",
        "nesbot/carbon": "^1.21",
        "doctrine/dbal": "^2.5",
        "sendgrid/sendgrid": "~4.0",
        "easyrdf/easyrdf": "^0.9.1",
        "ml/json-ld": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
