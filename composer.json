{
    "name": "StadGent/laravel_site_opening-hours",
    "description": "An open source web application to create calendars that represent openinghours of public services.",
    "keywords": ["framework", "laravel"],
    "license": "AGPLv3",
    "type": "Web application",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/weconnectdata/ics-parser"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "laravel/framework": "5.4.*",
        "santigarcor/laratrust": "3.0.*",
        "johngrogg/ics-parser": "^2.1.5",
        "nesbot/carbon": "^1.28",
        "doctrine/dbal": "^2.5",
        "easyrdf/easyrdf": "^0.9.1",
        "ml/json-ld": "^1.0",
        "pda/pheanstalk": "^3.1",
        "digipolisgent/robo-digipolis-laravel": "^0.1.3",
        "grasmash/yaml-expander": "^1.1",
        "digipolisgent/robo-digipolis-general": "0.1.1",
        "laravel/passport": "~2.0",
        "rlanvin/php-rrule": "^1.6",
        "fzaninotto/faker": "~1.4",
        "wildside/userstamps": "^0.5.3",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "laravel/browser-kit-testing": "1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
         "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/BrowserKitTestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize --force"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize --force"
        ],
        "post-autoload-dump": [
            "vendor/bin/robo precommit:install"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2.0"
        }
    }
}
