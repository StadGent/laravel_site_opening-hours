{
    "name": "stadgent/laravel_site_opening-hours",
    "description": "An open source web application to create calendars that represent openinghours of public services.",
    "license": "AGPLv3",
    "type": "project",
    "keywords": [
        "framework",
        "laravel"
    ],
    "require": {
        "php": ">=7.3.0",
        "digipolisgent/robo-digipolis-laravel": "^3.0",
        "doctrine/dbal": "^3.0",
        "easyrdf/easyrdf": "^1.0",
        "fakerphp/faker": "^1.23",
        "guzzlehttp/guzzle": "^7.3",
        "hedronium/spaceless-blade": "^3.0",
        "johngrogg/ics-parser": "^2.1.5",
        "laravel/framework": "^10",
        "laravel/passport": "^11.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.2",
        "ml/json-ld": "^1.0",
        "nesbot/carbon": "^2.0",
        "pda/pheanstalk": "~4.0",
        "rlanvin/php-rrule": "^1.6",
        "santigarcor/laratrust": "^8",
        "wildside/userstamps": "^2.3"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "laravel/browser-kit-testing": "^7.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^11.0"
    },
    "repositories": {
        "repman-digipolis": {
            "type": "composer",
            "url": "https://digipolis.repo.repman.io"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Openinghours\\": "robo"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/BrowserKitTestCase.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate --ansi"
        ]
    }
}
